{
  "name": "transform-to-tailwindcss",
  "type": "module",
  "version": "0.0.28",
  "packageManager": "pnpm@10.8.0",
  "description": "The tool that transform css to tailwindcss",
  "author": "Simon He",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/transformToTailwindcss#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Simon-He95/transformToTailwindcss.git"
  },
  "bugs": "https://github.com/Simon-He95/transformToTailwindcss/issues",
  "keywords": [
    "transform-to-tailwindcss",
    "tailwindcss"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "totailwindcss": "./cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup ./src/index.ts ./src/cli.ts",
    "dev": "npm run build -- --watch src",
    "format": "prettier --write --cache .",
    "lint": "eslint . --cache",
    "lint:fix": "pnpm run lint --fix",
    "play": "pnpm run -C playground dev",
    "play:build": "pnpm run -C playground build",
    "preview": "pnpm run -C playground preview",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.16.0",
    "@babel/core": "7.21.0",
    "@babel/eslint-parser": "^7.27.0",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.27.0",
    "@rollup/pluginutils": "^5.1.4",
    "@simon_he/colorize": "^0.0.1",
    "@types/babel__core": "^7.20.5",
    "@types/less": "^3.0.8",
    "@types/node": "^18.19.86",
    "@types/stylus": "^0.48.43",
    "@unocss/core": "^0.50.8",
    "@unocss/preset-uno": "^0.50.8",
    "@vue/babel-plugin-jsx": "^1.4.0",
    "@vue/compiler-sfc": "3.5.13",
    "bumpp": "^8.2.1",
    "eslint": "^9.24.0",
    "fast-glob": "^3.3.3",
    "find-up": "^7.0.0",
    "less": "^4.3.0",
    "less-plugin-module-resolver": "^1.0.3",
    "lint-staged": "^13.3.0",
    "magic-string": "^0.30.17",
    "picocolors": "^1.1.1",
    "prettier": "^3.5.3",
    "sass": "^1.86.3",
    "simple-git-hooks": "^2.12.1",
    "stylus": "^0.63.0",
    "transform-to-tailwindcss": "workspace:^",
    "transform-to-tailwindcss-core": "^0.0.20",
    "tsup": "^6.7.0",
    "tsx": "^3.14.0",
    "typescript": "^5.8.3",
    "unplugin": "^2.3.2",
    "vitest": "^2.1.9"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ],
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint . --fix"
  }
}
