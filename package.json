{
  "name": "transform-to-tailwindcss",
  "version": "0.0.22",
  "packageManager": "pnpm@8.6.8",
  "description": "The tool that transform css to tailwindcss",
  "author": "Simon He",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/transformToTailwindcss#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Simon-He95/transformToTailwindcss.git"
  },
  "bugs": "https://github.com/Simon-He95/transformToTailwindcss/issues",
  "keywords": [
    "transform-to-tailwindcss",
    "tailwindcss"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "totailwindcss": "./cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup ./src/index.ts ./src/cli.ts",
    "dev": "npm run build -- --watch src",
    "format": "prettier --write --cache .",
    "lint": "eslint . --cache",
    "lint:fix": "pnpm run lint --fix",
    "play": "pnpm run -C playground dev",
    "play:build": "pnpm run -C playground build",
    "preview": "pnpm run -C playground preview",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "fast-glob": "^3.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.21.0",
    "@rollup/pluginutils": "^5.0.2",
    "@simon_he/colorize": "^0.0.1",
    "@types/babel__core": "^7.20.0",
    "@types/less": "^3.0.3",
    "@types/node": "^18.0.0",
    "@types/stylus": "^0.48.38",
    "@unocss/preset-uno": "^0.50.4",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.18.0",
    "esno": "^0.16.3",
    "find-up": "^7.0.0",
    "lazy-js-utils": "^0.0.91",
    "less": "^4.2.0",
    "less-plugin-module-resolver": "^1.0.3",
    "lint-staged": "^15.2.2",
    "magic-string": "^0.30.10",
    "monaco-editor": "^0.48.0",
    "picocolors": "^1.0.0",
    "pnpm": "^9.0.5",
    "prettier": "^3.2.5",
    "rimraf": "^3.0.2",
    "sass": "^1.75.0",
    "simple-git-hooks": "^2.11.1",
    "stylus": "^0.63.0",
    "transform-to-tailwindcss": "workspace:^",
    "transform-to-tailwindcss-core": "^0.0.18",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^4.7.4",
    "unplugin": "^1.10.1",
    "vitest": "^1.5.0"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ],
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  }
}
