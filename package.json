{
  "name": "transform-to-tailwindcss",
  "version": "0.0.15",
  "packageManager": "pnpm@8.10.5",
  "description": "The tool that transform css to tailwindcss",
  "author": "Simon He",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/transformToTailwindcss#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Simon-He95/transformToTailwindcss.git"
  },
  "bugs": "https://github.com/Simon-He95/transformToTailwindcss/issues",
  "keywords": [
    "transform-to-tailwindcss",
    "tailwindcss"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "totailwindcss": "./cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup ./src/index.ts ./src/cli.ts",
    "dev": "npm run build -- --watch src",
    "format": "prettier --write --cache .",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix",
    "play": "pnpm run -C playground dev",
    "play:build": "pnpm run -C playground build",
    "preview": "pnpm run -C playground preview",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "fast-glob": "^3.3.2",
    "lazy-js-utils": "^0.0.75"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@babel/core": "^7.23.3",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.23.4",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/pluginutils": "^5.0.5",
    "@simon_he/colorize": "^0.0.1",
    "@types/babel__core": "^7.20.5",
    "@types/less": "^3.0.6",
    "@types/node": "^18.18.13",
    "@types/stylus": "^0.48.42",
    "@vue/babel-plugin-jsx": "^1.1.5",
    "bumpp": "^8.2.1",
    "eslint": "^8.54.0",
    "esno": "^0.16.3",
    "find-up": "^6.3.0",
    "less": "^4.2.0",
    "less-plugin-module-resolver": "^1.0.3",
    "lint-staged": "^13.3.0",
    "magic-string": "^0.30.5",
    "monaco-editor": "^0.36.1",
    "picocolors": "^1.0.0",
    "pnpm": "^8.10.5",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "sass": "^1.69.5",
    "stylus": "^0.59.0",
    "transform-to-tailwindcss": "workspace:^",
    "transform-to-tailwindcss-core": "^0.0.12",
    "tsup": "^6.7.0",
    "tsx": "^3.14.0",
    "typescript": "^4.9.5",
    "unplugin": "^1.5.1",
    "vitest": "^0.15.2"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ],
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
